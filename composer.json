{
	"name": "LAZ",
	"description": "Open Source social network made simple",
	"keywords": ["B2B Apps", "Metrics", "Social Network", "B2C Apps", "Community"],
	"license": "Eclipse Public License",
	"type": "project",
	"authors": [
		{
		"name": "Core Tech Labs",
		"email":"info@ctlabs.net",
		"homepage": "http://ctlabs.net"
		},
		{
			"name": "Rudy Jessop",
			"email": "rudyjessop@gmail.com",
			"homepage": "http://rudyjessop.com"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"laravel/framework": "5.1.*",
		"illuminate/html": "*",
		"intervention/image": "2.3.*",
		"predis/predis": "~1.0",
		"fzaninotto/faker": "^1.5",
		"aws/aws-sdk-php": "^3.9",
		"league/flysystem-aws-s3-v3": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"mockery/mockery": "~0.9.2"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"LAZ\\": "LAZ/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
